    {
      "type": "cpu",
      "key": "󰇺",
      "format": "{1}"
    },
    {
      "type": "memory",
      "key": "",
      "format": "{1} / {2}"
    },
    {
      "type": "custom",
      "format": " "
    },
    {
      "type": "custom",
      "format": "\u001b[40;30m███\u001b[41;31m███\u001b[42;32m███\u001b[43;33m███\u001b[44;34m███\u001b[45;35m███\u001b[46;36m███\u001b[47;37m███"
    },
    {
      "type": "custom",
      "format": " "
    }
  ]
