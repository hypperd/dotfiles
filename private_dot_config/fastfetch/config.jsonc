{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "kitty",
    "source": "~/.face",
    "padding": {
      "right": 2
    },
    "width": 32
  },
  "display": {
    "separator": "   "
  },
  "modules": [
    "title",
    {
      "type": "separator",
      "string": "━"
    },
    {
      "type": "os",
      "key": "󰍲"
    },
    {
      "type": "host",
      "key": "󰌢",
      "format": "{3}"
    },
    {
      "type": "kernel",
      "key": ""
    },
    {
      "type": "uptime",
      "key": "",
      "format": "{?1}{1}d {?}{?2}{2}h {?}{3}m"
    },
    {
      "type": "packages",
      "key": "󰏔"
    },
    {
      "type": "shell",
      "key": ""
    },
    {
      "type": "display",
      "key": "",
      "compactType": "original"
    },
    {
      "type": "wm",
      "key": ""
    },
    // {
    //   "type": "gpu",
