{
  "layer": "top",
  "position": "bottom",
  "margin-left": 8,
  "margin-bottom": 8,
  "margin-right": 8,
  "height": 36,
  "modules-left": [
    "custom/start",
    "sway/workspaces",
    "sway/mode",
    "mpris",
    "custom/backlight",
    "custom/updates",
    "systemd-failed-units",
    "sway/scratchpad",
    "privacy"
  ],
  "modules-center": [
    "sway/window"
  ],
  "modules-right": [
    "upower",
    "power-profiles-daemon",
    "pulseaudio",
    "group/system-info",
    "tray",
    "group/clock-notification"
  ],
  // modules left
  "custom/start": {
    "format": "ÔåÉ",
    "tooltip": false
  },
  "sway/workspaces": {
    "disable-scroll": true,
    "all-outputs": false,
    "format": "{icon}",
    "format-icons": {
      "11": "1",
      "12": "2",
      "13": "3",
      "14": "4",
      "15": "5",
      "16": "6",
      "17": "7",
      "18": "8",
      "19": "9",
      "21": "1",
      "22": "2",
      "23": "3",
      "24": "4",
      "25": "5",
      "26": "6",
      "27": "7",
      "28": "8",
      "29": "9"
    }
  },
  "sway/mode": {
    "format": "<span style=\"italic\">{}</span>"
  },
  "mpris": {
    "player": "spotify",
    "format": "{player_icon} {status_icon} {dynamic}",
    "tooltip-format": "{dynamic}",
    "format-paused": "{player_icon} {status_icon} <i>{dynamic}</i>",
    "dynamic-order": [
      "artist",
      "title"
    ],
    "dynamic-importance-order": [
      "artist",
      "title"
    ],
    "player-icons": {
      "default": "üéµ",
      "mpd": "ÔÄÅ",
      "spotify": "ÔÜº"
    },
    "status-icons": {
      "paused": "ÔÖÑ",
      "playing": "Ôäã"
    },
    "max-length": 26,
    "on-click-middle": "",
    "on-click-right": ""
  },
  "custom/backlight": {
    "tooltip": false,
    "format": "Û∞É† {}%",
    "exec": "exec ${XDG_CONFIG_HOME:-$HOME/.config}/waybar/scripts/backlight.py"
  },
  "custom/updates": {
    "format": "ÔÑπ {}",
    "return-type": "json",
    "interval": "once",
    "exec": "exec ${XDG_CONFIG_HOME:-$HOME/.config}/waybar/scripts/printupdates.sh",
    "on-click": "exec systemctl --user start checkupdates.service",
    "signal": 1
  },
  "privacy": {
    "icon-size": 12,
    "modules": [
      {
        "type": "screenshare",
        "tooltip": true,
        "tooltip-icon-size": 12
      },
      {
        "type": "audio-in",
        "tooltip": true,
        "tooltip-icon-size": 12
      }
    ]
  },
  "systemd-failed-units": {
    "hide-on-ok": true,
    "format": "ÔÅ± {nr_failed}",
    "system": true,
    "user": true
  },
  // modules center
  "sway/window": {
    "format": "{app_id}#{shell}#",
    "max-length": 50,
    "icon": false,
    "rewrite": {
      "##": "",
      ".*\\.([^.]*)": "$1",
      "(.*)#xdg_shell#": "$1",
      "(.*)#xwayland#": "$1 Ôç© "
    }
  },
  // modules right
  "sway/scratchpad": {
    "format": "{icon} {count}",
    "show-empty": false,
    "format-icons": [
      "", // 0
      "Ó≠ø", // 1
      "Ôãí" // > 1
    ],
    "tooltip": true,
    "tooltip-format": "{app}: {title}"
  },
  "power-profiles-daemon": {
    "format": "{icon}",
    "tooltip-format": "Power profile: {profile}",
    "tooltip": true,
    "format-icons": {
      "default": "ÔÉß",
      "performance": "ÔÉß",
      "balanced": "Û∞æÖ",
      "power-saver": "ÔÅ¨"
    }
  },
  "upower": {
    "model": "Logitech G703 LS",
    "icon-size": 16,
    "show-icon": true,
    "hide-if-empty": true,
    "format": "",
    "tooltip-format": "{percentage}",
    "tooltip": true
  },
  "pulseaudio": {
    "format": "{icon} {format_source} {volume}%",
    "format-muted": "Û∞üé {format_source}",
    "format-source": "ÔÑ∞",
    "format-source-muted": "ÔÑ±",
    "format-icons": {
      "headphone": "Û∞ãã",
      "default": [
        "ÔÄ¶",
        "ÔÄß",
        "ÔÄ®"
      ]
    },
    "on-click": "exec runapp -- pavucontrol"
  },
  "memory": {
    "interval": 2,
    "format": "Óâ¶ {used:0.1f}G"
  },
  "cpu": {
    "interval": 2,
    "format": "Ôíº {usage}%",
    "tooltip": false
  },
  "group/system-info": {
    "orientation": "horizontal",
    "modules": [
      "memory",
      "cpu"
    ]
  },
  "tray": {
    "spacing": 8
  },
  "clock": {
    "format": "Ôê∫ {:%H:%M}",
    "format-alt": "Ôëï {:%d/%m/%y}",
    "tooltip": false
  },
  "custom/notification": {
    "tooltip": true,
    "format": "{icon}",
    "format-icons": {
      "notification": "Û±Ö´",
      "none": "Û∞Çú",
      "dnd-notification": "Û∞Ç†",
      "dnd-none": "Û∞™ì",
      "inhibited-notification": "Û∞Çõ",
      "inhibited-none": "Û∞™ë",
      "dnd-inhibited-notification": "Û∞Çõ",
      "dnd-inhibited-none": "Û∞™ë"
    },
    "return-type": "json",
    "exec": "exec swaync-client -swb",
    "on-click": "exec swaync-client -d -sw",
    "escape": true
  },
  "group/clock-notification": {
    "orientation": "horizontal",
    "modules": [
      "clock",
      "custom/notification"
    ]
  }
}
